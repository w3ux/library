{
  "name": "@w3ux/extension-assets-source",
  "license": "GPL-3.0-only",
  "version": "0.0.11",
  "type": "module",
  "scripts": {
    "clear": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "declare": "tsc .build/**/*{.ts,.tsx} --declaration --allowJs --jsx react-jsx --emitDeclarationOnly --outDir .build",
    "build": "tsup .build/**/*{.ts,.tsx} --format esm --target es2022 --dts --no-splitting"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "@types/react": "^18",
    "react": "^18"
  }
}
